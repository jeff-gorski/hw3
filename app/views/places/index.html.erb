<h1>Places I've Been</h1>

<ul style="list-style: none; padding: 0;">
  <% for place in @places %>
    <li style="margin-bottom: 16px;">
      <a href="/places/<%= place["id"] %>">
        <%= place["name"] %>
      </a>
    </li>
  <% end %>
</ul>

<p style="margin-top: 64px;">
  <a href="/places/new">New Place</a>
</p>